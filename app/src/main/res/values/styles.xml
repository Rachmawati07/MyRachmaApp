<?xml version="1.0" encoding="utf-8"?>

<!-- Merupakan style yang digunakan untuk memberikan gaya pada textView, Background
     - Mempunyai name berupa AppTheme
-->
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!-- Mempunyai name berupa CustomTheme-->
    <style name="CustomTheme">
        <item name="android:background">@color/colorAccent</item>
    </style>

    <!-- Mempunyai name berupa CustomThemeNews-->
    <style name="CustomThemeNews">
        <item name="android:background">#B18F64</item>
    </style>
</resources>